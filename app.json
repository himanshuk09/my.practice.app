{
    "expo": {
        "name": "eeC",
        "slug": "eeC",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/applogo.png",
        "scheme": "enexion",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/applogo.png",
                "backgroundColor": "#cccccc"
            },
            "package": "com.himanshuk13.enexion",
            "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
        },
        "web": {
            "bundler": "metro",
            "output": "single",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-localization",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/appicon.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff",
                    "imageWidth": 200
                }
            ],
            [
                "expo-screen-orientation",
                {
                    "initialOrientation": "DEFAULT"
                }
            ],
            [
                "expo-secure-store",
                {
                    "configureAndroidBackup": true
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "868a5d67-6fc7-4f30-8df9-c0c8f7cf56e6"
            }
        }
    }
}
